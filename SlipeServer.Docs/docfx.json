{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [
            "**SlipeServer.Packets.csproj",
            "**SlipeServer.Net.csproj"
          ]
        }
      ],
      "dest": "api/networking",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "tocNamespaceStyle": "compactnested"
    },
    {
      "src": [
        {
          "src": "../",
          "files": [
            "**SlipeServer.Server.csproj"
          ]
        }
      ],
      "dest": "api/server",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "tocNamespaceStyle": "compactnested"
    },
    {
      "src": [
        {
          "src": "../",
          "files": [
            "**SlipeServer.Scripting.csproj",
            "**SlipeServer.Lua.csproj"
          ]
        }
      ],
      "dest": "api/optionals/scripting",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "tocNamespaceStyle": "compactnested"
    },
    {
      "src": [
        {
          "src": "../",
          "files": [
            "**SlipeServer.Physics.csproj"
          ]
        }
      ],
      "dest": "api/optionals/physics",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "src": [
        {
          "src": "../",
          "files": [
            "**SlipeServer.LuaControllers.csproj"
          ]
        }
      ],
      "dest": "api/optionals/lua-controllers",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "tocNamespaceStyle": "compactnested"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "xrefService": [      
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "template"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}